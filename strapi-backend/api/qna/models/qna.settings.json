{
  "kind": "collectionType",
  "collectionName": "qnas",
  "info": {
    "name": "Qna",
    "description": "Questions and answers"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "text",
      "required": true
    },
    "user": {
      "via": "qnas",
      "model": "auth"
    },
    "lecture": {
      "model": "lecture",
      "via": "qnas",
      "description": "The lecture this QnA is related to"
    },
    "answers": {
      "collection": "answer",
      "via": "qna",
      "description": "List of answers related to this QnA"
    }
  }
}
